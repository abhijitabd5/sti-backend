{
  "name": "earth-movers-backend",
  "version": "1.0.0",
  "description": "Earth Movers Training Academy Backend API",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop"
  },
  "keywords": [
    "training",
    "academy",
    "api",
    "express",
    "mysql"
  ],
  "author": "Earth Movers Training Academy",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "mysql2": "^3.11.0",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.17.1",
    "redis": "^5.8.1",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "slugify": "^1.6.6",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
